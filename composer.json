{
    "name": "xitedev/wiretables",
    "description": "Wiretables",
    "keywords": [
        "laravel",
        "wiretables"
    ],
    "homepage": "https://github.com/xitedev/wiretables",
    "license": "MIT",
    "authors": [
        {
            "name": "Xite",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0",
        "livewire/livewire": "^3.5",
        "spatie/laravel-package-tools": "^1.16",
        "spatie/laravel-query-builder": "^6.2",
        "xitedev/searchable": "@dev || ^2.2",
        "xitedev/wireforms": "@dev || ^2.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "nunomaduro/collision": "^8.5",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Xite\\Wiretables\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xite\\Wiretables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xite\\Wiretables\\WiretablesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
